
<div id="m_ImageResource" class="portfolio-modal modal fade" role="dialog" aria-hidden="true">
    <div class="modal-content">
        <div class="close-modal visible-lg" data-dismiss="modal">
            <div class="lr">
                <div class="rl">
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="modal-body">
                        <h2 class="visible-lg"> @Resources.Common.title_Resource</h2>
                        <hr class="star-primary" />
                        <img id="img_Resource" src="" class="img-responsive img-centered" alt="alt" />
                        <ul class="list-inline item-details">
                            <li>
                                @Resources.Common.lbl_FileName:
                                <strong>
                                    <span id="sR_FileName" class="RemarkLabel"></span>
                                </strong>
                            </li>
                            <li>
                                @Resources.Common.lbl_Createdby:
                                <strong>
                                    <span id="sR_CreatedBy" class="RemarkLabel"></span>
                                </strong>
                            </li>
                            <li>
                                @Resources.Common.lbl_CreatedOn:
                                <strong>
                                    <span id="sR_CreatedOn" class="RemarkLabel"></span>
                                </strong>
                            </li>
                        </ul>
                        <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i>Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>